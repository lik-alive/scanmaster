{
  "name": "lik_alive/scanmaster",
  "description": "Shared environment for scanned documents",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "jenssegers/blade": "^1.4",
    "php-di/php-di": "^6.3",
    "slim/psr7": "^1.5",
    "slim/slim": "^4.9",
    "php-di/slim-bridge": "^3.2",
    "vlucas/phpdotenv": "^5.4",
    "slim/csrf": "^1.2"
  },
  "autoload": {
    "files": [
      "./src/helpers.php"
    ],
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "start": "(cd public/ && php -S localhost:8080 index.php)"
  }
}
